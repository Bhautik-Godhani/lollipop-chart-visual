                    "category": {
                        "min": 1,
                        "max": 1
                    },
                    "measure": {
                        "max": 2
                    }
                }
            ],
            "categorical": {
                "categories": {
                    "select": [
                        {
                            "for": {
                                "in": "category"
                            }
                        },
                        {
                            "for": {
                                "in": "subCategory"
                            }
                        }
                    ],
                    "dataReductionAlgorithm": {
                        "top": {
                            "count": 2000
                        }
                    }
                },
                "values": {
                    "select": [
                        {
                            "bind": {
                                "to": "measure1"
                            }
                        },
                        {
                            "bind": {
                                "to": "measure2"
                            }
                        },
                        {
                            "bind": {
                                "to": "tooltip"
                            }
                        }
                    ]
                }
            }
        }
